  <section>
        <div class="container mt-5">
            <div class="row">
                {{#each cars}}
                <div class="col-md-3 ">
                    <div class="card" style="width: 16rem; height: 25rem; p-5">
                        <img src="{{this.image}}"
                            class="card-img-top" alt="...">
                        <div class="card-body">
                            <h5 class="card-title">{{this.name}}</h5>
                            <p class="card-text">{{this.prize}}</p>
                            <p class="card-text">{{this.feature}}</p>
                            <a href="#" class="btn btn-primary">Buy</a>
                        </div>
                    </div>
                </div>
                {{/each}}
                </div>
            </div>
        </div>
    </section>
